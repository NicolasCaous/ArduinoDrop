#define nota(notaTocada, duracao) \
    ({ \
        if(mudarMusica) { \
            mudarMusica = false; \
            return; \
        } \
        tone(buzzerpin, notaTocada); \
        delay(duracao); \
    })

void pausa(float duracao) {
    noTone(buzzerpin);
    delay(duracao);
}
    
bool mudarMusica;
int musicaAtual;

unsigned long milis;

void setup()
{

    pinMode(buzzerpin, OUTPUT);
    pinMode(buttonpin, INPUT);
    attachInterrupt(digitalPinToInterrupt(buttonpin), Musica, RISING);
    
    mudarMusica = false;
    musicaAtual = 5;
    milis = millis();

}

void Musica() {
    if(millis() - milis > 100) {
        mudarMusica = true;
        if(musicaAtual == 5)
            musicaAtual = 0;
        else
            musicaAtual++;
        milis = millis();
    }
}

void loop()
{

    if(musicaAtual == 0)
        tocarFlight();
    else if(musicaAtual == 1) 
        tocarValkyries();
    else if(musicaAtual == 2)
        tocarFur();
    else if(musicaAtual == 3)
        tocarFinalCountdown();
    else if(musicaAtual == 4)
        tocarMissionImpossible();
    else
        tocarNaruto();
        
}

